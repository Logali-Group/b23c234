<mvc:View
        controllerName="b23c234.controller.App"
        xmlns="sap.m"
        xmlns:mvc="sap.ui.core.mvc">

        <Shell id="shell">

            <App id="app">

                <pages>

                    <Page>  

                        <headerContent>

                                <Button id="helloDialogHeader"
                                        icon="sap-icon://action"
                                        press="onOpenDialogHeader"
                                        class="sapUiSmallMarginEnd"/>
                        
                        </headerContent>

                        <content>

                            <mvc:XMLView id="HelloPanel" 
                                        viewName="b23c234.view.HelloPanel"
                                        type="XML"/>

                            <mvc:XMLView id="InvoicesList"
                                        viewName="b23c234.view.InvoicesList"
                                        type="XML"/>

                        </content>

                    </Page>

                </pages>

            </App>

        </Shell>

</mvc:View>